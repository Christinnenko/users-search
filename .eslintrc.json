{
  "extends": ["eslint:recommended", "prettier", "react-app", "react-app/jest"],
  "plugins": ["prettier", "css-in-js"],
  "rules": {
    "camelcase": ["error", { "properties": "never" }],
    "prettier/prettier": "error",
    "eqeqeq": ["error", "always"],
    "no-unused-vars": ["error", { "varsIgnorePattern": "^React$" }]
  }
}
